l(225, 100%, 94%);\r\n  --bright-blue: hsl(245, 75%, 52%);\r\n  --very-pale-blue: hsl(225, 100%, 98%);\r\n  --desaturated-blue: hsl(224, 23%, 55%);\r\n  --dark-blue: hsl(223, 47%, 23%);\r\n}\r\n\r\nbody {\r\n  font-family: \"Red Hat Display\", sans-serif;\r\n  background-color: var(--pale-blue);\r\n  text-align: center;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  min-height: 100vh;\r\n}\r\n\r\nimg {\r\n  max-width: 100%;\r\n}\r\n\r\nbutton {\r\n  font-family: inherit;\r\n}\r\n\r\np {\r\n  font-size: 16px;\r\n  line-height: 1.6;\r\n  color: var(--desaturated-blue);\r\n  margin: 0;\r\n}\r\n\r\nh2 {\r\n  font-size: 16px;\r\n  margin: 0 0 0 0;\r\n}\r\n\r\n/* background image */\r\n\r\n.bg-image {\r\n  background: url(../images/pattern-background-mobile.svg) no-repeat\r\n    center/cover;\r\n  height: 400px;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  z-index: -1;\r\n}\r\n\r\n.btn {\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 10px;\r\n  font-weight: bold;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n  transition: all 0.2s ease-in-out;\r\n}\r\n\r\n.btn-change {\r\n  background-color: transparent;\r\n  color: var(--secondary-bright-blue);\r\n  text-decoration: underline;\r\n}\r\n\r\n.btn-primary {\r\n  background-color: var(--bright-blue);\r\n  color: #fff;\r\n  padding: 10px;\r\n  margin: 10px;\r\n}\r\n\r\n.btn-primary:hover,\r\n.btn-change {\r\n  opacity: 0.8;\r\n}\r\n\r\n.btn-secondary {\r\n  background-color: transparent;\r\n  color: var(--desaturated-blue);\r\n}\r\n\r\n.btn-secondary:hover {\r\n  color: hsl(223, 47%, 23%);\r\n}\r\n\r\n/* main div */\r\n.container {\r\n  margin: 20px;\r\n  padding-bottom: 20px;\r\n  background-color: #fff;\r\n  border-radius: 15px;\r\n  box-shadow: 2px 5px 5px var(--desaturated-blue);\r\n}\r\n\r\n.container p.para {\r\n  padding: 0 30px;\r\n}\r\n\r\n.container h1 {\r\n  color: hsl(223, 47%, 23%);\r\n  margin: 10px 0;\r\n}\r\n\r\n.container img.hero {\r\n  width: 100%;\r\n  border-radius: 15px 15px 0 0;\r\n}\r\n\r\n.container .prices {\r\n  margin: 30px auto;\r\n  padding: 10px;\r\n  background-color: var(--pale-blue);\r\n  width: 90%;\r\n  border-radius: 5px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n.container .prices article {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.container .buttons {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.container .prices article img {\r\n  margin-right: 10px;\r\n}\r\n\r\n/* Attribution */\r\n\r\n.attribution {\r\n  font-size: 11px;\r\n  text-align: center;\r\n}\r\n\r\n.attribution a {\r\n  color: hsl(228, 45%, 44%);\r\n}\r\n\r\n@media screen and (min-width: 768px) {\r\n  .bg-image {\r\n    background: url(../images/pattern-background-desktop.svg) no-repeat\r\n      center/cover;\r\n    height: 50vh;\r\n  }\r\n\r\n  .container {\r\n    width: 500px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"ed57a7b2c688e133778914a509873fb1","cacheData":{"env":{}}}